---

import Layout from '../layouts/layout.astro';

---



  <Layout>
    

<header  class="top-fixed">
  <div class="top-row">
    <h1 class="brand-logo">
      Tanishq
      
    </h1>
  </div>
  <!-- Navigation -->
  <nav class="nav">
    <a href="#home">Home</a>
    <a href="#collections">Collections</a>
    <a href="#new-launch">New Launch</a>
    <a href="#services">All Jewellery</a>
    <a href="#contact">Contact</a>
  </nav>
</header>





<main>

<div  id="home" class="carousel-container">
  <button class="prev"><i class="fas fa-arrow-left"></i></button>

  <div class="carousel-track">
    <div class="slide"><img src="tanishq/diamond.jpg" alt=""></div>
    <div class="slide"><img src="tanishq/gold.jpg" alt=""></div>
  </div>

  <button class="next"><i class="fas fa-arrow-right"></i>
 </button>
</div>








<section id="services" class="services">
  <h2>We provide what you love</h2>
  <p class="title">Discover timeless designs, crafted with elegance and care, just for you.</p>

  <div class="services-grid">
    <!-- Rings -->
    <div class="service-card">
      <img src="tanishq/ring.jpg" alt="Rings">
      <span class="badge">Trending</span>
    
      <a href="https://www.tanishq.co.in/shop/rings" target="_blank">
        <button class="see-more">See More</button>
      </a>
    </div>

    <!-- Pendants -->
    <div class="service-card">
      <img src="tanishq/pendant.jpg" alt="Pendants">
      <span class="badge">Trending</span>
    
      <a href="https://www.tanishq.co.in/shop/pendants" target="_blank">
        <button class="see-more">See More</button>
      </a>
    </div>

    <!-- Earrings -->
    <div class="service-card">
      <img src="tanishq/earring.jpg" alt="Earrings">
      <span class="badge">Trending</span>
      
      <a href="https://www.tanishq.co.in/shop/earrings" target="_blank">
        <button class="see-more">See More</button>
      </a>
    </div>

    <!-- Bracelets -->
    <div class="service-card">
      <img src="tanishq/bracelate.jpg" alt="Bracelets">
      <span class="badge">Trending</span>
   
      <a href="https://www.tanishq.co.in/shop/bracelets" target="_blank">
        <button class="see-more">See More</button>
      </a>
    </div>

    <!-- Pendants -->
    <div class="service-card">
      <img src="tanishq/necklace.jpg" alt="Pendants">
      <span class="badge">Trending</span>
      
      <a href="https://www.tanishq.co.in/shop/pendants" target="_blank">
        <button class="see-more">See More</button>
      </a>
    </div>

    

  </div>
</section>




<section id="collections" class="collections">
  <h2>Our Collections</h2>
   <span class="tagline">Discover timeless designs crafted with love</span>
  <div class="collections-grid">
    
    <div class="collection-card">
      <img src="tanishq/bridal.jpg" alt="Bridal Collection">
      <div class="overlay">
        <h3>Bridal Collection</h3>
        <p class="tagline">Upto 30% OFF - Limited Time</p>
        <a href="https://www.tanishq.co.in/shop/bridal-jewellery" target="_blank">
          <button>Explore Collection</button>
        </a>
      </div>
    </div>

    <div class="collection-card">
      <img src="tanishq/dailywear.jpg" alt="Daily Wear Gold">
      <div class="overlay">
        <h3>Daily Wear Gold</h3>
        <p class="tagline">Buy 1 Get 1 Free on select pieces</p>
        <a href="https://www.tanishq.co.in/shop/daily-wear" target="_blank">
          <button>Explore Collection</button>
        </a>
      </div>
    </div>

    <div class="collection-card">
      <img src="tanishq/diamond2.jpg" alt="Diamond Elegance">
      <div class="overlay">
        <h3>Diamond Elegance</h3>
        <p class="tagline">Flat 20% OFF - Limited Time</p>
         <a href="https://www.tanishq.co.in/search?search-button=&q=diamond&lang=en_IN" target="_blank">
          <button>Explore Collection</button>
        </a>
      </div>
    </div>

    <div class="collection-card">
      <img src="tanishq/gifting.jpg" alt="Gifting Special">
      <div class="overlay">
        <h3>Gifting Special</h3>
        <p class="tagline">Flat 20% OFF + Free Gift Wrap</p>
       <a href="https://www.tanishq.co.in/shop/gifts" target="_blank">
          <button>Explore Collection</button>
        </a>
      </div>
    </div>

  </div>
</section>



